{
  "plugins":["plugins/mardown"],
  "recurseDepth": 10,
  "source": {
    "include": "dist",
    "includepattern": ".js$",
    "excludepattern": "(node_modules|doc)"
  },
  "sourceType":"module",
  "tag":{
    "allowUnkownTags": true,
    "dictionaries":["jsdoc", "closure"]
  },
"templates":{
  "cleverLinks": true,
  "monospaceLinks": true
},
"opts": {
  "destination":"./docs/",
  "recurse": true
}
}